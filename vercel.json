{
  "version": 2,
    
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "https://bayava-server-oexv.vercel.app"
      }
    }
  ]
}